{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        { "src": "/storybook", "dest": "/storybook/index.html" },
        { "src": "/(.*)", "dest": "/styleguide/$1" }
    ]
}
